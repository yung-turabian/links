infixl 6 `coerce`;

class Coerce : (a, b) {
    sig `coerce` : (a) -> b;
}

instance Coerce : (Int, String) {
    `coerce` : intToString;
}

instance Coerce : (Int, Float) {
    `coerce` : intToFloat;
}

instance Coerce : (Bool, Int) {
    `coerce` :  fun(b) {if (b) 1 else 0};
}

instance Coerce : ([Int], Int) {
    `coerce` : max_list;
}

`coerce` [4, 5, 3, 9]
